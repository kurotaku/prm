<%= render 'shared/error_messages', object: f.object %>

<div class="form-group">
  <p><%= f.label :name %></p>
  <%= f.text_field :name, class: 'form-control half' %>
</div>

<div class="form-group">
  <p><%= f.label :code %></p>
  <%= f.text_field :code, class: 'form-control half' %>
</div>

<div class="form-group">
  <p><%= f.label :image %></p>
  <%= f.file_field :image, id: 'file-field', class: 'file-field', data: {image: 'upload-file'}  %>
</div>

<div class="form-group">
  <p><%= f.label :agency_price %></p>
  <%= f.text_field :agency_price, class: 'form-control half' %>
</div>

<div class="form-group">
  <p><%= f.label :agency_incentive %></p>
  <%= f.text_field :agency_incentive, class: 'form-control half' %>
</div>

<div class="form-group">
  <p><%= f.label :agency_description %></p>
  <%= f.text_area :agency_description, class: 'form-control', rows: 6 %>
</div>

<div class="form-group">
  <p><%= f.label :introduction_price %></p>
  <%= f.text_field :introduction_price, class: 'form-control half' %>
</div>

<div class="form-group">
  <p><%= f.label :introduction_incentive %></p>
  <%= f.text_field :introduction_incentive, class: 'form-control half' %>
</div>

<div class="form-group">
  <p><%= f.label :introduction_description %></p>
  <%= f.text_area :introduction_description, class: 'form-control', rows: 6 %>
</div>

<div class="form-group">
  <p><%= f.label :wholesale_price %></p>
  <%= f.text_field :wholesale_price, class: 'form-control half' %>
</div>

<div class="form-group">
  <p><%= f.label :wholesale_description %></p>
  <%= f.text_area :wholesale_description, class: 'form-control', rows: 6 %>
</div>